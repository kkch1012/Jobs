# 운영환경용 환경변수 예시 (env.production.example)
# 이 파일을 EC2에서 .env로 복사하고 실제 값들로 교체하세요

# ⚠️ 필수: 실제 API 키로 교체
OPENROUTER_API_KEY=your_actual_openrouter_api_key_here

# ⚠️ 필수: 강력한 시크릿 키로 교체 (최소 32자)
SECRET_KEY=your_very_strong_secret_key_for_production_at_least_32_chars

# 운영 데이터베이스 설정 (기본값 또는 변경)
POSTGRES_USER=myuser
POSTGRES_PASSWORD=mypassword
POSTGRES_DB=jobs

# MongoDB 운영 설정 (기본값 또는 변경)
MONGO_INITDB_ROOT_USERNAME=admin
MONGO_INITDB_ROOT_PASSWORD=yourpassword
MONGO_INITDB_DATABASE=jobs_db

# 운영환경 설정
ENVIRONMENT=production
ACCESS_TOKEN_EXPIRE_MINUTES=60
BATCH_SCHEDULE_HOUR=2
BATCH_SCHEDULE_MINUTE=0

# Redis 비밀번호
REDIS_PASSWORD=redispassword 